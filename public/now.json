{
  "version": 2,
  "name": "petful",
  "alias": "petful",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
      },
      {
        "src": "^\/(.*\\.(png|ico|jpg|xml))$",
        "dest": "/public/$1"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}